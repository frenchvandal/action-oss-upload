{
  "name": "action-oss-upload",
  "description": "This GitHub Action uploads generated files to an OSS bucket",
  "main": "lib/main.js",
  "engines": {
    "node": "^15.12.0",
    "npm": "^7.6.3"
  },
  "scripts": {
    "build": "tsc && ncc build --target es2020",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint --fix ./src/**/*.ts",
    "format": "prettier --write **/*.ts **/*.json **/*.yaml",
    "format:check": "prettier --check **/*.ts **/*.json **/*.yaml",
    "pre-checkin": "concurrently \"npm:format\" \"npm:build\""
  },
  "repository": "github:frenchvandal/action-oss-upload",
  "keywords": [
    "actions",
    "hugo",
    "build",
    "package",
    "website",
    "aliyun",
    "alioss"
  ],
  "author": "frenchvandal",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/glob": "^0.1.1",
    "ali-oss": "^6.15.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.0.7",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "@vercel/ncc": "^0.27.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "typescript": "^4.2.2",
    "typescript-formatter": "^7.2.2"
  }
}
