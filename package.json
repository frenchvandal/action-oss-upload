{
  "name": "action-oss-upload",
  "description": "This GitHub Action uploads generated files to an OSS bucket",
  "main": "lib/main.js",
  "engines": {
    "node": ">=12.22.1",
    "npm": ">=6.14.12"
  },
  "scripts": {
    "build": "tsc && ncc build",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint --fix ./src/**/*.ts",
    "format": "prettier --write **/*.ts **/*.json **/*.yaml",
    "format:check": "prettier --check **/*.ts **/*.json **/*.yaml",
    "pre-checkin": "concurrently \"npm:lint:fix\" \"npm:format\" \"npm:build\"",
    "prepare": "husky install"
  },
  "repository": "github:frenchvandal/action-oss-upload",
  "keywords": [
    "actions",
    "hugo",
    "build",
    "package",
    "website",
    "aliyun",
    "alioss"
  ],
  "author": "frenchvandal",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.7",
    "@actions/glob": "^0.1.1",
    "ali-oss": "^6.15.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/ali-oss": "^6.0.8",
    "@types/concurrently": "^6.0.1",
    "@types/eslint": "^7.2.10",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^15.0.1",
    "@types/prettier": "^2.2.3",
    "@types/standard-version": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vercel/ncc": "^0.28.4",
    "concurrently": "^6.0.2",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "standard-version": "^9.2.0",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.{js,json,md,ts,yaml}": "prettier --write"
  }
}
